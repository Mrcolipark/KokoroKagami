{
  "name": "kokoro-kagami",
  "version": "1.0.0",
  "description": "心を映す占いアプリ - 四柱推命・星座・タロット",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace KokoroKagami.xcworkspace -scheme KokoroKagami -configuration Release archive"
  },
  "keywords": ["占い", "四柱推命", "星座", "タロット", "日本"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/stack": "^6.3.0",
    "@reduxjs/toolkit": "^1.9.0",
    "react-redux": "^8.1.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^13.9.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.6.0",
    "react-native-screens": "^3.21.0",
    "react-native-keychain": "^8.1.0",
    "react-native-async-storage": "^1.19.0",
    "react-native-push-notification": "^8.1.0",
    "react-native-share": "^9.4.0",
    "react-native-linear-gradient": "^2.8.0",
    "react-native-device-info": "^10.9.0",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.45.0",
    "lottie-react-native": "^6.0.0",
    "swiss-ephemeris": "^2.10.0",
    "bazijs": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.0",
    "typescript": "^5.1.0",
    "jest": "^29.5.0",
    "@testing-library/react-native": "^12.1.0"
  }
}
